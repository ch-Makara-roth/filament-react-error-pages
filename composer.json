{
    "name": "makararoth/filament-react-error-pages",
    "description": "A Laravel package that provides React-based custom error pages with Filament support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Makara Roth",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "filament/filament": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MakaraRoth\\FilamentReactErrorPages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakaraRoth\\FilamentReactErrorPages\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MakaraRoth\\FilamentReactErrorPages\\Providers\\FilamentReactErrorPagesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

